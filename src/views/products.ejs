
        <table class="table table-dark">
          <thead>
            <tr>
              <th>ID</th>
              <th>Name</th>
              <th>Description</th>
              <th>Price</th>
              <th>Pic</th>
              <th>Edit</th>
            </tr>
          </thead>
          <tbody>
           <% products.forEach(product=>{ %>

              <tr>
                <th><%= product.id %></th>
                <td><%= product.name %></td>
                <td><%= product.desc %></td>
                <td><%= product.price %></td>
                <td><img src="<%= product.imgurl %>" ></td>
                <td><a href="/update-product/<%= product.id %>" class="btn btn-warning">Update</a></td>
              </tr>

            <% }) %>
          </tbody>
        </table>
